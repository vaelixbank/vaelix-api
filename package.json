{
  "name": "vaelix-api",
  "version": "1.0.0",
  "description": "Vaelix Bank API based on Weavr Multi API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "ts-node app/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "prepare": "husky install",
    "migrate": "node migrate_encrypt_api_keys.js",
    "db:schema": "node scripts/inject-schema.js",
    "db:validate": "node scripts/validate_schema.sql",
    "db:seed": "node scripts/seed-database.js",
    "db:backup": "node scripts/db-backup-restore.js backup",
    "db:restore": "node scripts/db-backup-restore.js restore",
    "db:health": "node scripts/db-health-check.js",
    "docker:build": "docker build -t vaelix-api .",
    "docker:run": "docker run -p 3000:3000 vaelix-api",
    "docs:build": "typedoc",
    "clean": "rm -rf dist coverage",
    "init-master-iban": "node init_master_account_iban.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaelixbank/vaelix-api.git"
  },
  "keywords": [
    "banking",
    "api",
    "weavr"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "17.2.3",
    "express": "5.1.0",
    "express-rate-limit": "8.1.0",
    "helmet": "8.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "5.0.3",
    "@types/express-rate-limit": "5.1.3",
    "@types/node": "24.8.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "bugs": {
    "url": "https://github.com/vaelixbank/vaelix-api/issues"
  },
  "homepage": "https://github.com/vaelixbank/vaelix-api#readme"
}
